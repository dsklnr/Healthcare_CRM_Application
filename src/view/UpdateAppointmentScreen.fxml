<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="900.0" prefWidth="800.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UpdateAppointmentController">
    <children>
        <Label layoutX="37.0" layoutY="58.0" style="-fx-text-fill: #1C6CF6;" text="Schedule An Appointment">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
        <Label layoutX="37.0" layoutY="134.0" style="-fx-text-fill: #1C6CF6;" text="Appointment ID">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <TextField fx:id="appointmentId" disable="true" editable="false" layoutX="165.0" layoutY="132.0" prefHeight="25.0" prefWidth="174.0" promptText="Disabled" />
        <Label layoutX="39.0" layoutY="316.0" style="-fx-text-fill: #1C6CF6;" text="Title">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <TextField fx:id="title" layoutX="167.0" layoutY="314.0" prefHeight="25.0" prefWidth="174.0" />
        <Label layoutX="39.0" layoutY="440.0" style="-fx-text-fill: #1C6CF6;" text="Location">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="39.0" layoutY="198.0" style="-fx-text-fill: #1C6CF6;" text="Doctor">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="37.0" layoutY="375.0" style="-fx-text-fill: #1C6CF6;" text="Type">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="39.0" layoutY="494.0" style="-fx-text-fill: #1C6CF6;" text="Start Date &amp; Time">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="39.0" layoutY="554.0" style="-fx-text-fill: #1C6CF6;" text="End Date &amp; Time">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="39.0" layoutY="257.0" style="-fx-text-fill: #1C6CF6;" text="Patient">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="39.0" layoutY="615.0" style="-fx-text-fill: #1C6CF6;" text="Description">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Button layoutX="701.0" layoutY="840.0" mnemonicParsing="false" onAction="#onSaveAddAppointment" prefHeight="31.0" prefWidth="79.0" style="-fx-background-radius: 45; -fx-background-color: #00F6FF; -fx-border-radius: 45; -fx-border-color: #FAF9F6;" text="Save">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button layoutX="597.0" layoutY="840.0" mnemonicParsing="false" onAction="#onCancelAddAppointment" prefHeight="25.0" prefWidth="79.0" style="-fx-background-radius: 45; -fx-background-color: #00F6FF; -fx-border-radius: 45; -fx-border-color: #FAF9F6;" text="Cancel">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <TextField fx:id="location" layoutX="167.0" layoutY="438.0" prefHeight="25.0" prefWidth="174.0" />
        <TextField fx:id="type" layoutX="165.0" layoutY="373.0" prefHeight="25.0" prefWidth="174.0" />
        <TextArea fx:id="description" layoutX="165.0" layoutY="622.0" prefHeight="175.0" prefWidth="536.0" />
        <ComboBox fx:id="doctorNameComboBox" layoutX="167.0" layoutY="196.0" prefHeight="25.0" prefWidth="174.0" />
        <DatePicker fx:id="startDate" layoutX="167.0" layoutY="492.0" />
        <DatePicker fx:id="endDate" layoutX="165.0" layoutY="552.0" />
        <ComboBox fx:id="startHourComboBox" layoutX="402.0" layoutY="492.0" prefWidth="150.0" promptText="Select Hour" />
        <ComboBox fx:id="startMinuteComboBox" layoutX="603.0" layoutY="492.0" prefWidth="150.0" promptText="Select Minute" />
        <ComboBox fx:id="endHourComboBox" layoutX="402.0" layoutY="552.0" prefWidth="150.0" promptText="Select Hour" />
        <ComboBox fx:id="endMinuteComboBox" layoutX="603.0" layoutY="552.0" prefWidth="150.0" promptText="Select Minute" />
        <ComboBox fx:id="patientComboBox" layoutX="167.0" layoutY="255.0" prefHeight="25.0" prefWidth="174.0" />
    </children>

</AnchorPane>
